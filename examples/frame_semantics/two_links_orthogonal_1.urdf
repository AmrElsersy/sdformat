<?xml version="1.0"?>
<robot name="two_links_orthogonal_1">
  <link name="world"/>
  <joint name="joint_world" type="fixed">
    <parent link="world"/>
    <child link="linkA"/>
  </joint>
  <link name="linkA">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="1"/>
      <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
    </inertial>
  </link>

  <joint name="jointAB" type="continuous">
    <origin xyz="0.1 0 0" rpy="0 0 0"/>
    <parent link="linkA"/>
    <child link="linkB"/>
    <axis xyz="0 1 0"/>
  </joint>

  <link name="linkB">
    <inertial>
      <origin xyz="0 0 0.1" rpy="0 0 0"/>
      <mass value="1"/>
      <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
    </inertial>
  </link>
</robot>
