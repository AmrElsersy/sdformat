<?xml version="1.0"?>
<sdf version="1.5">
  <model name="two_links_orthogonal_2">
    <link name="link1">
      <pose>0 0 0 0 0 0</pose>
    </link>

    <joint name="joint12" type="revolute">
      <pose>0 0 0.1 0 0 0</pose>
      <parent>link1</parent>
      <child>link2</child>
      <axis>
        <xyz>0 1 0</xyz>
      </axis>
    </joint>

    <link name="link2">
      <pose relative_to="joint12"/>
      <inertial>
        <pose>0.1 0 0 0 0 0</pose>
      </inertial>
    </link>

    <joint name="joint_world" type="fixed">
      <parent>world</parent>
      <child>link1</child>
    </joint>
  </model>
</sdf>
