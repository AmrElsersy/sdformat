              <stddev>0.2</stddev>
            </noise>
          </x>
          <y>
            <noise type="gaussian">
              <mean>1.2</mean>
              <stddev>2.3</stddev>
            </noise>
          </y>
          <z>
            <noise type="gaussian">
              <mean>3.4</mean>
              <stddev>5.6</stddev>
